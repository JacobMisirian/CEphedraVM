#ifndef _INSTRUCTIONS_H_
#define _INSTRUCTIONS_H_

#define INST_ADD 0x0
#define INST_CALL 0x1
#define INST_HCF 0x2
#define INST_JMP 0x3
#define INST_LB 0x4
#define INST_LI 0x5
#define INST_LW 0x6
#define INST_MOD 0x7
#define INST_MOV 0x8
#define INST_POP 0x9
#define INST_PUSH 0xA
#define INST_RET 0xB
#define INST_SB 0xC
#define INST_SW 0xD
#define INST_SUB 0xE

#endif
